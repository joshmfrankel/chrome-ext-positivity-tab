{
    "manifest_version": 2,
    "name": "Positivity Tab",
    "description": "A new tab that shows positive affirming messages",
    "version": "1.0.0",
    "chrome_url_overrides": {"newtab": "new-tab.html"},
    "options_page": "/options/options.html",
    "permissions": [
      "activeTab",
      "https://ajax.googleapis.com/"
    ],
    "icons": {
      "32": "positivity-tab-icon-32.png",
      "64": "positivity-tab-icon-64.png"
    }
}
